{
  "name": "react-webpack-demo",
  "version": "1.0.0",
  "description": "Example of webapp using react redux and hot reloading",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run start-prod",
    "start-dev": "better-npm-run start-dev",
    "dev": "concurrent --kill-others \"npm run start-dev\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaotuni/react-webpack-demo"
  },
  "homepage": "https://github.com/xiaotuni/react-webpack-demo",
  "keywords": [
    "babel",
    "bundle-loader",
    "eslint",
    "hot reloading",
    "react",
    "react-router-dom",
    "react-hot-reloader",
    "redux",
    "webpack"
  ],
  "author": "xiaotuni <xiaotuni@gmail.com> (http://github.com/xiaotuni)",
  "license": "ISC",
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --config webpack/dev.config.js  --color --progress --hot",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-prod": {
      "command": "webpack --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-redux-saga": "^0.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-pxtorem": "^4.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "1.x",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "superagent": "^3.6.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
